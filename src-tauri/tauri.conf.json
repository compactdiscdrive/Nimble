{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "nimble",
  "mainBinaryName": "nimble",
  "version": "0.1.0",
  "identifier": "xyz.beige.nimble",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "nimble",
        "width": 1024,
        "height": 768,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
        "icons/nimble-32x32.png",
        "icons/nimble-64x64.png",
        "icons/nimble-128x128.png",
        "icons/nimble-icon.ico",
        "icons/nimble-icon.icns"
    ],
    "resources": [
    "nimble.desktop"
  ]
  }
}
